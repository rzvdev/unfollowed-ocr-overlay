{
  "CsvImport": {
    "HasHeader": true,
    "Delimiter": ",",
    "MaxRows": 200000
  },
  "Scan": {
    "TargetFps": 4
  },
  "Preprocess": {
    "Profile": "Default",
    "Contrast": 1.0,
    "Sharpen": 0.0
  },
  "Ocr": {
    "LanguageTag": "en",
    "MinTokenConfidence": 0.0,
    "CharacterWhitelist": "abcdefghijklmnopqrstuvwxyz0123456789._@"
  },
  "Stabilizer": {
    "WindowSizeM": 3,
    "RequiredK": 2,
    "ConfidenceThreshold": 0.60,
    "AllowUncertainHighlights": false
  }
}
