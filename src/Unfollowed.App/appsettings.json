{
  "CsvImport": {
    "HasHeader": true,
    "Delimiter": ",",
    "MaxRows": 200000
  },
  "Scan": {
    "TargetFps": 4
  },
  "Capture": {
    "FrameDumpEnabled": false,
    "FrameDumpEveryNFrames": 0,
    "FrameDumpOutputDirectory": "frame_dumps"
  },
  "Preprocess": {
    "Profile": "Default",
    "Contrast": 1.0,
    "Sharpen": 0.0
  },
  "Overlay": {
    "AlwaysOnTop": true,
    "ClickThrough": true,
    "ShowBadgeText": true,
    "ShowOcrText": false
  },
  "Ocr": {
    "LanguageTag": "en",
    "MinTokenConfidence": 0.0,
    "CharacterWhitelist": "abcdefghijklmnopqrstuvwxyz0123456789._@",
    "AssumedTokenConfidence": 0.85
  },
  "Stabilizer": {
    "WindowSizeM": 5,
    "RequiredK": 3,
    "ConfidenceThreshold": 0.70,
    "AllowUncertainHighlights": false
  }
}
